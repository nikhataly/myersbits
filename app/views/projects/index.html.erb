

<h1>Projects</h1>


<%= search_form_for @search do |s| %>  
  <div class="field">
  <%= s.label :title_or_description_or_location_or_skills_required_cont, "Search" %>
  <%= s.text_field :title_or_description_or_location_or_skills_required_cont %>
  </div>

  <div class="submit"><%= s.submit "Submit"%></div>

<% end %>
<br>
<div class='container'>

  <%= render @projects %>

</div>

<%= will_paginate @projects, renderer: BootstrapPagination::Rails %>

<%= link_to 'Create Project', new_project_path %>


