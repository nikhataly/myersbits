

<h1>Projects</h1>


<%= form_tag projects_path, method: :get, authentication: false, id: 'search-form' do %>
  <%= text_field_tag :search, params[:search] %>
  <%= submit_tag "Search" %>
<% end %>
<br>
<div class='container'>
<div id="projects">
  <%= render @projects %>
</div>
</div>

<%= will_paginate @projects, renderer: BootstrapPagination::Rails %>

<%= link_to 'Create Project', new_project_path %>


