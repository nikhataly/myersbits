<% if current_user.nil? %>
<div class="jumbotron">
  <h1>Hello, world!</h1>
  <p>...</p>
  <p><a class="btn btn-primary btn-lg" href="#" role="button">Learn more</a></p>
</div>
<% end %>

<p>Please log-in</p>



<h1>Projects</h1>


<%= form_tag projects_path, method: :get, authentication: false, id: 'search-form' do %>
  <%= text_field_tag :search, params[:search] %>
  <%= submit_tag "Search" %>
<% end %>
<br>

<div id="projects">
  <%= render @projects %>
</div>

<%= will_paginate @projects, renderer: BootstrapPagination::Rails %>

<%= link_to 'Create Project', new_project_path %>


