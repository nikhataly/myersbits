
<div class="well">
<h1><%= @user.name %></h1>

<div class="avatar">
  <%= image_tag @user.avatar.url(:medium) %>
</div>


<p>
 <strong>My MBTI: <%= @user.personality.mbti %></strong>


 </p>

<p>
 <strong>My projects </strong>


 </p>


<section>
    <button id="current-location"> Find projects near me</button>

</section>

<section>
  <%= form_tag projects_path, method: :get do %>
  <%= label_tag :search, "Find projects nearby" %>
  <%= search_field_tag :search, params[:search] %>
  <%= submit_tag "Search" %>
  <% end %>
</section>

<section>
<h3>My Projects</h3>
  <%= render @projects %>
</section>
</div>